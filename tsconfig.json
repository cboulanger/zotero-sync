{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "importHelpers": true,
    "target": "es2018",
    "disableSizeLimit": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": false,
    "sourceMap": false,
    "downlevelIteration": true,
    "lib": [ "es2018" ],
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ]
  }
}
