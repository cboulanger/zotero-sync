{
  "name": "@retorquere/zotero-sync",
  "version": "1.0.22",
  "description": "One-way sync of Zotero libraries",
  "scripts": {
    "tsc": "tsc -d",
    "docs": "typedoc",
    "prepublishOnly": "pnpm install && npm run tsc && npm run docs",
    "go": "npm run tsc && npm run docs && npm version patch && npm publish --access public",
    "ncu": "ncu -u && pnpm i && git add package.json pnpm-lock.yaml && git commit -m ncu",
    "postversion": "git push --follow-tags"
  },
  "main": "index.js",
  "typings": "./typings/zotero.d.ts",
  "author": "Emiliano Heyns",
  "license": "AGPL",
  "keywords": [
    "zotero",
    "zotero-sync"
  ],
  "devDependencies": {
    "@types/node": "^14.14.41",
    "ejs": "^3.1.6",
    "eslint": "^7.24.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "json-stringify-pretty-compact": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "json-store.js",
    "package.json",
    "pnpm-lock.yaml",
    "tsconfig.json",
    "typings/zotero.d.ts",
    "index.d.ts",
    "json-store.d.ts"
  ]
}
